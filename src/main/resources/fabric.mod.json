{
  "schemaVersion": 1,
  "id": "working-scheduler",
  "version": "${version}",

  "name": "Working Scheduler",
  "description": "Library mod that provides an working alternative to the Minecraft scheduler.",
  "authors": [
    "Fudge"
  ],
  "contact": {
    "homepage": "https://github.com/natanfudge/Working-Scheduler",
    "sources": "https://github.com/natanfudge/Working-Scheduler"
  },

  "license": "The MIT License",
  "environment": "*",
  "entrypoints": {
    "main": [
      "scheduler.internal.WorkingSchedulerKt::init"
    ],
    "client": [
      {
        "value": "scheduler.internal.WorkingSchedulerKt::initClient"
      }
    ]
  },
  "depends": {
    "fabricloader": ">=0.4.0",
    "fabric": "*",
    "fabric-language-kotlin": "*",
    "fabricdrawer": ">=1.1.1"
  },
  "custom": {
    "modmenu:api": true
  }
}
